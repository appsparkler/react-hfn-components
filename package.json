{
  "name": "@appsparkler/react-hfn-components",
  "version": "0.0.44",
  "license": "MIT",
  "main": "./dist/react-hfn-components.min.js",
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged; bundlesize; eslint src;",
      "post-merge": "(git-branch-is master && yarn version --patch || echo 'branch is not master')"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint --fix"
  },
  "scripts": {
    "storybook": "start-storybook",
    "start": "webpack-dev-server --config webpack.config.studio.js",
    "build": "webpack -p --config webpack.config.dist.js; bundlesize",
    "lint": "eslint src",
    "test": "jest test",
    "bundlesize": "bundlesize",
    "build:publish": "yarn build; yarn publish",
    "docs-clean": "rimraf docs",
    "docs-build": "jsdoc -c ./jsdoc.config.js -r -d ./docs",
    "docs-serve": "yarn docs-clean; yarn docs-build; yarn serve docs",
    "heroku-postbuild": "yarn webpack --config webpack.config.dist-studio.js"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-a11y": "^5.3.9",
    "@storybook/addon-docs": "^5.3.9",
    "@storybook/addon-storysource": "^5.3.9",
    "@storybook/react": "^5.3.9",
    "@uifabric/icons": "^7.3.20",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.4.1",
    "bundlesize": "^0.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.18.0",
    "firebase": "^7.7.0",
    "git-branch-is": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "ink-docstrap": "^1.3.2",
    "jest": "^25.1.0",
    "jquery": "^3.4.1",
    "jsdoc": "^3.6.3",
    "lint-staged": "^10.0.8",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "office-ui-fabric-react": "^7.105.1",
    "popper.js": "^1.16.1",
    "postcss-loader": "^3.0.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0",
    "uuid": "^3.4.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2"
  },
  "dependencies": {
    "croppie": "^2.6.4",
    "express": "^4.17.1"
  }
}
